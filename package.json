{
  "name": "preskor",
  "description": "Web-first crypto football prediction market app",
  "version": "1.0.0",
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/node": "^22.0.0",
    "husky": "^9.1.7",
    "syncpack": "^13.0.4",
    "turbo": "^1.10.0",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">=1.21.19"
  },
  "packageManager": "yarn@1.21.19",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "dev": "turbo run dev",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "syncpack:format": "syncpack format",
    "test": "turbo run test",
    "prepare": "husky"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ]
}
